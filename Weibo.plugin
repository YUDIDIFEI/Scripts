// ==UserScript==
// @ScriptName        微博去广告
// @Author            @ddgksf2013
// @TgChannel         https://t.me/ddgksf2021
// @Function          Block AD
// @ScriptTime        2022-10-20
// @ScriptURL         https://github.com/ddgksf2013/Rewrite/raw/master/AdBlock/Weibo.conf
// ==/UserScript==


[MITM]
hostname = api.weibo.cn, mapi.weibo.com, *.uve.weibo.com, new.vip.weibo.cn,

[Script]
# 微博去广告以及去除各部分推广模块 - cherish
http-response ^https?://m?api\.weibo\.c(n|om)/2/(cardlist|searchall|page|messageflow|statuses/(unread_)?friends(/|_)timeline|groups/timeline|statuses/(container_timeline|unread_hot_timeline|extend|video_mixtimeline|unread_topic_timeline)|profile/(me|statuses)|video/(community_tab|remind_info|tiny_stream_video_list)|checkin/show|\!/live/media_homelist|comments/build_comments|container/get_item|search/(finder|container_timeline|container_discover)) script-path=https://github.com/YUDIDIFEI/Scripts/blob/master/weibo_main.js, requires-body=true, tag=推广模块

# 删除微博开屏广告 - cherish
http-response ^https?://(sdk|wb)app\.uve\.weibo\.com(/interface/sdk/sdkad.php|/wbapplua/wbpullad.lua) script-path=https://github.com/YUDIDIFEI/Scripts/blob/master/weibo_launch.js, requires-body=true, tag=开屏广告

# 自定义tab皮肤
http-response ^https://api.weibo.cn/2/!/client/light_skin script-path=https://github.com/YUDIDIFEI/Scripts/blob/master/weibo_main.js, requires-body=true, tag=皮肤
# 非会员设置tab皮肤 - cherish
http-response ^https://new.vip.weibo.cn/littleskin/preview script-path=https://github.com/YUDIDIFEI/Scripts/blob/master/weibo_main.js, requires-body=true, tag=tab皮肤
